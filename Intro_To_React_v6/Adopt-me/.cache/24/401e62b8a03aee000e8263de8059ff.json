s[0],\r\n      });\r\n    } catch (error) {\r\n      console.warn(error);\r\n    }\r\n  }\r\n\r\n  render() {\r\n    if (this.state.loading) {\r\n      return <h2>Loading...</h2>;\r\n    }\r\n\r\n    const { name, description, animal, breed, city, state, images } = this.state;\r\n\r\n    return (\r\n      <div className=\"details\">\r\n        <Carousel>{images}</Carousel>\r\n        <div>\r\n          <h2>Hi</h2>\r\n          <h1>{name}</h1>\r\n          <h2>\r\n            {animal} - {breed} - {`${city}, ${state}`}\r\n          </h2>\r\n\r\n          <ThemeContext.Consumer>\r\n            {([theme]) => {\r\n              return <button style={{ backgroundColor: theme }}>Adopt {name}</button>;\r\n            }}\r\n          </ThemeContext.Consumer>\r\n\r\n          <p>{description}</p>\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nconst DetailsWithRouter = withRouter(Details);\r\n\r\nexport default function DetailsWithErrorBoundary(props) {\r\n  return (\r\n    <ErrorBoundary>\r\n      <DetailsWithRouter {...props} />\r\n    </ErrorBoundary>\r\n  );\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"e324ad98831636936801277bb989273e","cacheData":{"env":{}}}